{
  "name": "MoegirlViewer",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "yarn adb:reverse && react-native start",
    "test": "jest",
    "lint": "eslint .",
    "postinstall": "jetifier -r",
    "android": "react-native run-android && yarn start",
    "clear": "cd android && ./gradlew clean",
    "build": "yarn clear && cd android && ./gradlew assembleRelease && cd ../",
    "build:test": "yarn clear && react-native run-android --variant release",
    "adb:reverse": "adb reverse tcp:8081 tcp:8081",
    "adb:menu": "adb shell input keyevent 82",
    "adb:push": "adb push android/app/build/outputs/apk/release/app-release.apk sdcard",
    "adb:restart": "adb kill-server && adb start-server",
    "adb:install": "adb install -r android/app/build/outputs/apk/release/app-release.apk",
    "adb:uninstall": "adb uninstall com.moegirlviewer",
    "adb:reinstall": "yarn adb:uninstall && yarn adb:install"
  },
  "dependencies": {
    "@koharubiyori/react-native-cookies": "^3.3.0",
    "@koharubiyori/react-native-orientation": "^3.1.4",
